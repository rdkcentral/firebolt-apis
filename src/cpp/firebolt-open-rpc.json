							"fontEdgeColor": "#7F7F7F",
							"fontOpacity": 100,
							"backgroundColor": "#000000",
							"backgroundOpacity": 100,
							"textAlign": "center",
							"textAlignVertical": "middle",
							"windowColor": "white",
							"windowOpacity": 50
						},
						"preferredLanguages": [
